{
  "name": "playstation-game-manager",
  "main": "app/app.ts",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@nativescript/core": "8.4.0",
    "@nativescript/local-notifications": "6.1.1",
    "@nativescript/tailwind": "2.0.1",
    "@nativescript/theme": "3.0.2",
    "nativescript-sqlite": "2.8.6"
  },
  "devDependencies": {
    "@nativescript/android": "8.4.0",
    "@nativescript/preview-cli": "1.0.14",
    "@nativescript/stackblitz": "0.0.8",
    "@nativescript/types": "8.4.0",
    "@nativescript/webpack": "5.0.13",
    "tailwindcss": "3.2.4",
    "typescript": "4.8.4"
  },
  "scripts": {
    "preview": "setup-nativescript-stackblitz && ns preview",
    "android": "ns build android --release --key-store-path playstation.keystore --key-store-password android --key-store-alias android --key-store-alias-password android --copy-to ./playstation.apk",
    "ios": "ns run ios",
    "clean": "ns clean",
    "setup": "setup-nativescript-stackblitz"
  },
  "stackblitz": {
    "startCommand": "setup-nativescript-stackblitz && ns preview"
  }
}
